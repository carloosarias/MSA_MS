<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="root_hbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AnalysisReportFX">
   <children>
      <VBox>
         <children>
            <TabPane tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Reportes De Análisis">
                     <content>
                        <HBox prefHeight="100.0" prefWidth="200.0" />
                     </content></Tab>
                <Tab text="Tipos de Análisis">
                     <content>
                        <HBox>
                           <children>
                              <VBox>
                                 <children>
                                    <TableView fx:id="analysistype_tableview">
                                      <columns>
                                        <TableColumn fx:id="analysistypeid_column" prefWidth="75.0" resizable="false" text="ID" />
                                        <TableColumn fx:id="analysistypename_column" prefWidth="120.0" resizable="false" text="Nombre" />
                                          <TableColumn fx:id="analysistypedescription_column" prefWidth="200.0" resizable="false" text="Descripción" />
                                          <TableColumn fx:id="analysistypefactor_column" prefWidth="100.0" resizable="false" text="Factor" />
                                          <TableColumn fx:id="analysistypeoptimal_column" prefWidth="100.0" resizable="false" text="Óptimo" />
                                      </columns>
                                       <VBox.margin>
                                          <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                                       </VBox.margin>
                                    </TableView>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </content></Tab>
              </tabs>
            </TabPane>
            <HBox>
               <children>
                  <Button mnemonicParsing="false" text="Nuevo Reporte De Análisis">
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="addanalysistype_button" mnemonicParsing="false" text="Agregar Tipo de Análisis">
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
